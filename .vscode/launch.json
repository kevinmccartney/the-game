{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug bin script",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/bin/${input:pickScript}",
      "console": "integratedTerminal",
      "justMyCode": true,
      "args": ["['__no-changes']"]
    }
  ],
  "inputs": [
    {
      "id": "pickScript",
      "description": "Select script to debug",
      "type": "pickString",
      "options": [
        "edfi",
        "find-changed-api-projects",
        "has-changed-api-projects"
      ],
      "default": "find-changed-api-projects"
    }
  ]
}
